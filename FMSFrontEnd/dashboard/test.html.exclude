    <script>




        google.load('visualization', '1', { packages: ['corechart'] });
        google.setOnLoadCallback(drawChart);


        //callback function
        function drawChart() {

            //Then retrieve
            var localObj = JSON.parse(sessionStorage.getItem("setSessionstorageValueEngine"));

            var move = localObj.Move;
            var idle = localObj.Idle;
            var stop = localObj.Stop;

            var data = google.visualization.arrayToDataTable([
                  ['Engine Status', 'Percentage'],
                  ['MOVE', move],
                  ['IDLE', idle],
                  ['STOP', stop],
            ]);


            var options = {
                title: 'Engine Status',
                titleTextStyle: { color: 'white', fontSize: '12', bold: 'true' },
                tooltip: { showColorCode: true },
                is3D: true,
                region: 'TH',
                displayMode: 'Engine Status',
                backgroundColor: '#2F343B',
                legend: { position: 'bottom', textStyle: { color: 'white', fontSize: 12 }, maxLines: 3 },
                //chartArea: { left: 50, top: 0, width: '50%', height: '75%' },
                chartArea: { left: 10, top: 20, width: '100%' },
                colors: ['#00CC00', '#FFA500', '#FF0000']


            };

            var chart = new google.visualization.PieChart(document.getElementById('chartdiv'));
            chart.draw(data, options);
        }


        function drawIgnitionChart() {

            //Then retrieve
            var localObj = JSON.parse(sessionStorage.getItem("setSessionstorageValueIgnition"));

            var on = localObj.On;
            var off = localObj.Off;


            var data = google.visualization.arrayToDataTable([
                  ['Ignition', 'Percentage'],
                  ['On', on],
                  ['Off', off]
            ]);


            var options = {
                title: 'Ignition',
                titleTextStyle: { color: 'white', fontSize: '12', bold: 'true' },
                tooltip: { showColorCode: true },
                is3D: true,
                region: 'TH',
                displayMode: 'Ignition',
                backgroundColor: '#2F343B',
                legend: { position: 'bottom', textStyle: { color: 'white', fontSize: 12 }, maxLines: 3 },
                //chartArea: { left: 50, top: 0, width: '50%', height: '75%' },
                chartArea: { left: 10, top: 20, width: '100%' },
                colors: ['#4169E1', 'red']


            };

            var chart = new google.visualization.PieChart(document.getElementById('chartdivIgnition'));
            chart.draw(data, options);
        }


    </script>


